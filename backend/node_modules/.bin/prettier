#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/bin/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/node_modules:/home/MyProject/Company-Assignment/backend/node_modules:/home/MyProject/Company-Assignment/node_modules:/home/MyProject/node_modules:/home/node_modules:/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/prettier/bin/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/prettier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/bin/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/prettier@3.0.3/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/.pnpm/node_modules:/home/MyProject/Company-Assignment/backend/node_modules:/home/MyProject/Company-Assignment/node_modules:/home/MyProject/node_modules:/home/node_modules:/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/prettier/bin/node_modules:/home/MyProject/Company-Assignment/backend/node_modules/prettier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../prettier/bin/prettier.cjs" "$@"
fi
